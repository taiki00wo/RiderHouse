<h1>Riderhouse#index</h1>
<p>Find me in app/views/riderhouse/index.html.erb</p>

<table class="table">
  <thead>
    <tr>
      <th>名称</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @riderhouses.each do |r| %>
    <tr>
      <th><%= link_to r.name, riderhouse_path(r) %></th>
      <th>
        <%= link_to '編集', edit_riderhouse_path(r), class: 'btn btn-primary' %>
        <%= link_to '削除', riderhouse_path(r), method: :delete, class: 'btn btn-danger' %>
      </th>
    <% end %>
  </tbody>
</table>

